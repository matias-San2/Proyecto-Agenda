{
  "name": "aws-cognito-jwt-login",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "serverless offline",
    "deploy": "serverless deploy --stage dev --region us-east-1",
    "deploy:dev": "serverless deploy --stage dev --region us-east-1",
    "deploy:staging": "serverless deploy --stage staging --region us-east-1",
    "deploy:prod": "serverless deploy --stage prod --region us-east-1",
    "remove": "serverless remove",
    "info": "serverless info --verbose",
    "info:dev": "serverless info --stage dev",
    "info:prod": "serverless info --stage prod",
    "logs:worker": "serverless logs -f personalizationWorker -t",
    "logs:dlq": "serverless logs -f personalizationDLQWorker -t",
    "logs": "serverless logs -f",
    "test": "echo 'Tests pending' && exit 0",
    "test:local": "npm run dev",
    "start:localstack": "docker-compose up -d",
    "stop:localstack": "docker-compose down",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix",
    "validate": "serverless print --stage dev",
    "package": "serverless package --stage dev",
    "seed": "node scripts/seed.js"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.6.0",
    "serverless-scriptable-plugin": "^1.3.1"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.600.0",
    "@aws-sdk/client-dynamodb": "^3.932.0",
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/client-sns": "^3.896.0",
    "@aws-sdk/client-sqs": "^3.943.0",
    "@aws-sdk/lib-dynamodb": "^3.600.0",
    "@aws-sdk/util-dynamodb": "^3.932.0",
    "lambda-multipart-parser": "^1.0.1"
  },
  "optionalDependencies": {
    "dotenv": "^17.2.3"
  }
}
